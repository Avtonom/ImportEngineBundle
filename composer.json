{
  "name": "mathielen/import-engine-bundle",
  "type": "symfony-bundle",
  "description": "A generic import (and export) -engine that provides easy to use yet powerful features",
  "keywords": [
    "csv",
    "import"
  ],
  "homepage": "https://github.com/mathielen/ImportEngineBundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Markus Thielen",
      "email": "markus@logicx.de"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "mathielen/import-engine": ">=0.7",
    "symfony/framework-bundle": "~2.4"
  },
  "require-dev": {
    "symfony/console": ">=2.2",
    "logicx/glueframework": "dev-master",
    "phpunit/phpunit": "*",
    "sensio/generator-bundle": "~2.3"
  },
  "suggest": {
    "symfony/console": "If you want to use the ImportCommand",
    "logicx/glueframework": "If you want to use the ImportCommand"
  },
  "autoload": {
    "psr-0": {
      "Mathielen\\ImportEngineBundle": ""
    }
  },
  "target-dir": "Mathielen/ImportEngineBundle"
}
